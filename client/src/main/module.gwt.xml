<?xml version="1.0" encoding="UTF-8"?>
<module>
    <inherits name="com.google.gwt.user.User"/>
    <inherits name="com.gwtplatform.dispatch.rest.DispatchRest"/>
    <inherits name="com.gwtplatform.dispatch.rest.delegates.ResourceDelegate"/>
    <inherits name='com.gwtplatform.mvp.MvpWithEntryPoint'/>

    <source path=""/>

    <extend-configuration-property name="gin.ginjector.modules"
                                   value="io.itdraft.youfollowme.website.gin.ClientModule"/>

    <set-configuration-property name="gwtp.bootstrapper"
                                value="io.itdraft.youfollowme.website.AppBootstrapper"/>

    <!-- Only support recent browsers -->
    <set-property name="user.agent" value="ie10,gecko1_8,safari"/>

    <set-configuration-property name="CssResource.enableGss" value="true"/>
    <set-configuration-property name="CssResource.conversionMode" value="strict" />
</module>
