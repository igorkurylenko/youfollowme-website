<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <ui:with field="tokens" type="io.itdraft.youfollowme.website.place.PlaceNameTokens"/>
    <ui:with field="res" type="io.itdraft.youfollowme.website.resources.AppResources"/>

    <g:HTMLPanel ui:field="typeMenu" styleName="{res.style.navbar}">
        <g:Button ui:field="hashtagsButton" text="Hashtags" styleName="{res.style.navbarItem}"/>
        <g:Button ui:field="domainsButton" text="Domains" styleName="{res.style.navbarItem}"/>
        <g:Button ui:field="keywordsButton" text="Keywords" styleName="{res.style.navbarItem}"
                  visible="false"/>
        <g:Button ui:field="githubButton" text="Github" styleName="{res.style.navbarItem}"
                  visible="false"/>
        <g:Button ui:field="playButton" styleName="{res.style.navbarItem}">
            <i class="{res.style.icon}">&#xE037;</i>
        </g:Button>
        <g:Button ui:field="pauseButton" styleName="{res.style.navbarItem}" visible="false">
            <i class="{res.style.icon}">&#xE034;</i>
        </g:Button>
    </g:HTMLPanel>
</ui:UiBinder>